<!DOCTYPE HTML>

<html>
	<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>Rate My School</title>
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<meta property="og:title" content=""/>
	<meta property="og:image" content=""/>
	<meta property="og:url" content=""/>
	<meta property="og:site_name" content=""/>
	<meta property="og:description" content=""/>
	<meta name="twitter:title" content="" />
	<meta name="twitter:image" content="" />
	<meta name="twitter:url" content="" />
	<meta name="twitter:card" content="" />


	<link rel="stylesheet" href="/css/profile.css">

	<link href="https://fonts.googleapis.com/css?family=Lato:300,400,700" rel="stylesheet">


	<link rel="stylesheet" href="animate.css">
	<!-- Icomoon Icon Fonts-->
	<link rel="stylesheet" href="icomoon.css">
	<!-- Themify Icons-->
	<link rel="stylesheet" href="themify-icons.css">
	<!-- Bootstrap  -->
	<link rel="stylesheet" href="bootstrap.css">

	<!-- Magnific Popup -->
	<link rel="stylesheet" href="magnific-popup.css">

	<!-- Magnific Popup -->
	<link rel="stylesheet" href="bootstrap-datepicker.min.css">

	<!-- Owl Carousel  -->
	<link rel="stylesheet" href="owl.carousel.min.css">
	<link rel="stylesheet" href="owl.theme.default.min.css">

	<!-- Theme style  -->
	<link rel="stylesheet" href="style.css">

	<!-- Modernizr JS -->
	<script src="js/modernizr-2.6.2.min.js"></script>
	<!-- FOR IE9 below -->
	<!--[if lt IE 9]>
	<script src="js/respond.min.js"></script>
	<![endif]-->

	</head>
	<body>

	<div class="gtco-loader"></div>


	<!-- <div class="page-inner"> -->
	<nav class="gtco-nav" role="navigation">
		<div class="gtco-container">

			<div class="row">
				<div class="col-sm-4 col-xs-12">
					<div id="gtco-logo"><a href="/">Rate My School <em>.</em></a></div>
				</div>
				<div class="col-xs-8 text-right menu-1">
					<ul>
						<li><a href="./">Sign Out</a></li>
						<li><a href="/">Home</a></li>
					</ul>
				</div>
			</div>

		</div>
	</nav>

	<header id="gtco-header" class="gtco-cover gtco-cover-sm" role="banner" style="background-image: url(images/img_3.jpg)">
		<div class="overlay"></div>
		<div class="gtco-container">
			<div class="row">
				<div id="background" class="col-md-12 col-md-offset-0 text-center">
					<div class="margin">
						<div id="uiPic" class="col-md-12 mt-text animate-box" data-animate-effect="fadeInUp">
							<section class="profilePic">
								<section class="form">
									<section class="picUpload">
										<img id="myImg" src= "<%= user.photo %>" height="250" width="300">
									</section>
									<form action="/up" enctype="multipart/form-data" method="post">
									 <input id="pic" type="file" name="file-to-upload" class="btn btn-primary btn-block">
									 <input id="upload" type="submit" class="btn btn-primary btn-block" value="Submit">
									</form>
									<ul class="messages">
										<% if(userInfo[0]) {%>
										<li class="message">
											<span contenteditable="true"><strong>Name: </strong><%= userInfo[0].name %></span>
											<br>
											<span contenteditable="true"><strong>School: </strong><%= userInfo[0].school %></span>
											<span class="hide"><%= userInfo[0]._id %></span>
											<span><i class="fas fa-edit" aria-hidden="true"></i></span>
											<br>
											<span class="edit" aria-hidden="true">Update</span>
											<span class="delete" aria-hidden="true">Remove</span>
										</li>
										<% } %>
									</ul>
									<form class="information" action="/profile" method="POST">
										<input type="text" name="name" value="" placeholder="Your Name">
										<input type="text" name="school" value="" placeholder="Your School">
										<input type="submit" class="btn btn-primary btn-block" value="Submit">
									</form>

								</section>
								<section class="schoolLookUp">
									<form class="searchForm" action="/search" method="POST">
										<div class="row form-group">
											<div class="col-md-12">
												<input type="text" id="fullname" placeholder="School Name" name="schoolSearch" class="form-control">
											</div>
										</div>
										<div class="row form-group">
											<div class="col-md-12">
												<input type="submit" class="btn btn-primary btn-block" value="Submit">
											</div>
										</div>
									</form>
								</section>
							</section>
						</div>

					</div>

							<div class="tab-content">
								<div class="tab-content-inner active" data-content="signup">


								</div>
							</div>

				</div>
			</div>
	</header>


	<!-- </div> -->

	<div class="gototop js-top">
		<a href="#" class="js-gotop"><i class="icon-arrow-up"></i></a>
	</div>

	<!-- jQuery -->
	<script src="jquery.min.js"></script>
	<!-- jQuery Easing -->
	<script src="jquery.easing.1.3.js"></script>
	<!-- Bootstrap -->
	<script src="bootstrap.min.js"></script>
	<!-- Waypoints -->
	<script src="jquery.waypoints.min.js"></script>
	<!-- Carousel -->
	<script src="owl.carousel.min.js"></script>
	<!-- countTo -->
	<script src="jquery.countTo.js"></script>

	<!-- Stellar Parallax -->
	<script src="jquery.stellar.min.js"></script>

	<!-- Magnific Popup -->
	<script src="jquery.magnific-popup.min.js"></script>
	<script src="magnific-popup-options.js"></script>

	<!-- Datepicker -->
	<script src="bootstrap-datepicker.min.js"></script>


	<!-- Main -->
	<script src="main.js"></script>

	</body>
</html>
